<div style="text-align:center">
    <h2><%= t'.welcome' %></h2>
    <p><%= t '.services_html' %> </p>
</div>
<div class="row">
  <div class="col">
    <h1>DEVISE TEST</h1>
    <% if  user_signed_in? %>
      <p><%= current_user.email %></p>
    <% else %>
      <p> Powinieneś się zalogować</p>
    <% end %>
